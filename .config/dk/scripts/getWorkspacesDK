#!/bin/sh
ws=$(dkcmd status type=ws num=1) 

w1=${ws%%1*}
w2=${ws%%2*} && w2=${w2##*:}
w3=${ws%%3*} && w3=${w3##*:}
w4=${ws%%4*} && w4=${w4##*:}
w5=${ws%%5*} && w5=${w5##*:}
w6=${ws%%6*} && w6=${w6##*:}
w7=${ws%%7*} && w7=${w7##*:}

[ "$w1" = "A" ] || [ "$w1" = "I" ] && i1="" || i1=""
[ "$w2" = "A" ] || [ "$w2" = "I" ] && i2="" || i2=""
[ "$w3" = "A" ] || [ "$w3" = "I" ] && i3="" || i3=""
[ "$w4" = "A" ] || [ "$w4" = "I" ] && i4="" || i4=""
[ "$w5" = "A" ] || [ "$w5" = "I" ] && i5="" || i5=""
[ "$w6" = "A" ] || [ "$w6" = "I" ] && i6="" || i6=""
[ "$w7" = "A" ] || [ "$w7" = "I" ] && i7="" || i7=""
[ "$w1" = "a" ] && i1=""
[ "$w2" = "a" ] && i2=""
[ "$w3" = "a" ] && i3=""
[ "$w4" = "a" ] && i4=""
[ "$w5" = "a" ] && i5=""
[ "$w6" = "a" ] && i6=""
[ "$w7" = "a" ] && i7=""

dunstify -h string:x-dunst-stack-tag:workspace "$i1  $i2  $i3  $i4  $i5  $i6  $i7"
